{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/assets/**",
          "/svg/**"
        ]
      }
    },
    {
      "name": "styles",
      "installMode": "prefetch",
      "updateMode": "lazy",
      "resources": {
        "files": [
          "/global.css",
          "/theme/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [],
      "patterns": [
        "http://localhost:3000/(?!auth/ingresar).*"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "6h",
        "timeout": "5s"
      }
    }
  ]
}
